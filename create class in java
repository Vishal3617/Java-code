public class Animal {
  public void makeSound() {
    System.out.println("The animal makes a sound.");
  }
}

public class Cat extends Animal {
  @Override
  public void makeSound() {
    System.out.println("The cat barks.");
  }
}

public class Main {
  public static void main(String[] args) {
    Animal animal = new Animal();
    animal.makeSound();

    Cat cat = new Cat();
    cat.makeSound();
  }
}
